pipeline {
    environment {
        isRollbackSuccess = ''   
    }
    agent any
    tools {
      maven 'maven'
      jdk 'jdk1.8.0'
    }
    stages {
        stage('FindSuccessfulBuilds') {
            steps {
                script  {
                  def builds = []

                  def job = jenkins.model.Jenkins.instance.getItem("simple-dataops-rollback")
                  job.builds.each {
                  def build = it
                  if (it.getResult().toString().equals("SUCCESS")) {
                      it.badgeActions.each {
                               builds.add(build.displayName[1..-1])
                       }
                  }
                  }

                  builds.unique();
                  println builds
                  for(build in builds)  {
                    println build
                  }
                }
            }
        }
    }
}
